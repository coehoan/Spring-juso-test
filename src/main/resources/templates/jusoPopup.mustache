<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <title>Document</title>
</head>

<body onload="init();">
    <form id="form" name="form" method="post">
        {{#inputYn}}
        <input type="hidden" id="inputYn" name="inputYn" value="{{inputYn}}" />
        {{/inputYn}}
        {{#zipNo}}
        <input type="hidden" id="zipNo" name="zipNo" value="{{zipNo}}" />
        {{/zipNo}}
        {{#roadFullAddr}}
        <input type="hidden" id="roadFullAddr" name="roadFullAddr" value="{{roadFullAddr}}" />
        {{/roadFullAddr}}
        <input type="hidden" id="confmKey" name="confmKey" value="" />
        <input type="hidden" id="returnUrl" name="returnUrl" value="" />
        <input type="hidden" id="resultType" name="resultType" value="" />
        <!-- 해당시스템의 인코딩타입이 EUC-KR일경우에만 추가 START-->
        <!-- 
		<input type="hidden" id="encodingType" name="encodingType" value="EUC-KR"/>
		 -->
        <!-- 해당시스템의 인코딩타입이 EUC-KR일경우에만 추가 END-->
    </form>

    <script>

        function init() {
            let url = location.href;
            let confmKey = "U01TX0FVVEgyMDIyMDUxODE1MzAwMTExMjU4MzU=";//승인키
            // resultType항목 추가(2016.10.06)
            let resultType = "4"; // 도로명주소 검색결과 화면 출력유형, 1 : 도로명, 2 : 도로명+지번+상세보기(관련지번, 관할주민센터), 3 : 도로명+상세보기(상세건물명), 4 : 도로명+지번+상세보기(관련지번, 관할주민센터, 상세건물명)
            let inputYn = $("#inputYn").val();

            if (inputYn != "Y") {
                $("#confmKey").val(confmKey);
                $("#returnUrl").val(url);
                $("#resultType").val(resultType); // resultType항목 추가(2016.10.06)
                document.form.action = "https://www.juso.go.kr/addrlink/addrLinkUrl.do"; // 인터넷망
                //document.form.action="https://www.juso.go.kr/addrlink/addrMobileLinkUrl.do"; //모바일 웹인 경우, 인터넷망
                document.form.submit();
            } else {
                alert("else떴음");
                alert(inputYn);
                /** API 서비스 제공항목 확대 (2017.02) **/
                let roadFullAddr = $("#roadFullAddr").val();
                let roadAddrPart1 = $("#roadAddrPart1").val();
                let addrDetail = $("#addrDetail").val();
                let roadAddrPart2 = $("#roadAddrPart2").val();
                let engAddr = $("#engAddr").val();
                let jibunAddr = $("#jibunAddr").val();
                let zipNo = $("#zipNo").val();
                let admCd = $("#admCd").val();
                let rnMgtSn = $("#rnMgtSn").val();
                let bdMgtSn = $("#bdMgtSn").val();
                let detBdNmList = $("#detBdNmList").val();
                let bdNm = $("#bdNm").val();
                let bdKdcd = $("#bdKdcd").val();
                let siNm = $("#siNm").val();
                let sggNm = $("#sggNm").val();
                let emdNm = $("#emdNm").val();
                let liNm = $("#liNm").val();
                let rn = $("#rn").val();
                let udrtYn = $("#udrtYn").val();
                let buldMnnm = $("#buldMnnm").val();
                let buldSlno = $("#buldSlno").val();
                let mtYn = $("#mtYn").val();
                let lnbrMnnm = $("#lnbrMnnm").val();
                let lnbrSlno = $("#lnbrSlno").val();
                let emdNo = $("#emdNo").val();
                opener.jusoCallBack(roadFullAddr, roadAddrPart1, addrDetail, roadAddrPart2, engAddr, jibunAddr, zipNo, admCd,
                    rnMgtSn, bdMgtSn, detBdNmList, bdNm, bdKdcd, siNm, sggNm, emdNm, liNm, rn, udrtYn, buldMnnm, buldSlno, mtYn,
                    lnbrMnnm, lnbrSlno, emdNo);
                window.close();
            }
        }
    </script>

</body>

</html>